<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toonmoa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="툰모아"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NaverActivity" />
        <activity android:name=".FoxtoonActivity" />
        <activity android:name=".KtoonActivity" />
        <activity android:name=".DaumActivity" />
        <activity android:name=".SearchActivity" />

        <service android:name="AlertService" android:enabled="true"></service>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>